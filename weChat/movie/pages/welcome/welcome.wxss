page {
  background: #b3d4db;
  height: 100%;
}

.welcome-container {
  width: 100%;
  height: 100%;
}

.welcome-content {
  display: flex;
  padding: 200rpx 0;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
}

.logo-image {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.text {
  margin-top: 10px;
}

.go-button {
  display: block;
  font-size: 32rpx;
  margin-top: 150rpx;
  padding: 15rpx;
  border-radius: 8px;
  color: #405f80;
  font-weight: bold;
  height: 30px;
  line-height: 30px;
  border: 1px solid orangered;
}